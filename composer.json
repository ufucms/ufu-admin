    "authors": [
        {
            "name": "ufucms",
            "email": "ufucms@ufucms.com",
            "homepage": "https://github.com/ufucms"
        }
    ],
    "homepage": "https://github.com/ufucms/ufu-admin",
    "support": {
        "email": "ufucms@ufucms.com",
        "issues": "https://github.com/ufucms/ufucms/issues",
        "forum": "https://github.com/orgs/ufucms/discussions",
        "wiki": "https://doc.owladmin.com/"
    },
    "keywords": [
        "laravel",
        "ufu-admin",
        "admin",
        "amis"
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": ">=9",
        "ufucms/laravel-captcha": "*",
        "stancl/virtualcolumn": "^1.3",
        "symfony/yaml": "^3.4|^4.0|^5.0|^6.0|^7.0",
        "slowlyo/laravel-support": "*",
        "ext-zip": "*",
        "ext-gd": "*"
    },
    "autoload": {
        "psr-4": {
            "Slowlyo\\OwlAdmin\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Slowlyo\\OwlAdmin\\AdminServiceProvider"
            ],
            "aliases": {
                "OwlAdmin": "Slowlyo\\OwlAdmin\\Facades\\OwlAdmin"
            }
        }
    }
}
